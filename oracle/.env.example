# Prophezy Oracle Service Environment Variables
# Copy this file to .env in the oracle directory

# AI Service API Keys (at least one required)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Oracle Service Configuration
PORT=8000
HOST=0.0.0.0

# Redstone Oracle Configuration
REDSTONE_API_KEY=your_redstone_api_key_here
REDSTONE_NODE_URL=https://api.redstone.finance

# Chainlink Configuration (if needed)
CHAINLINK_NODE_URL=

# Database (optional - for storing resolution history)
DATABASE_URL=postgresql://user:password@localhost:5432/prophezy

# Logging
LOG_LEVEL=INFO
